{
  "name": "mica",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html  --log-level info",
    "dev:kaios2": "parcel src/index.html --log-level info --target kaios2",
    "build:kaios2": "npm run clean && export BROWSERSLIST_ENV=kaios2 && parcel build --no-source-maps --no-cache --target kaios2",
    "build:kaios3": "npm run clean && export BROWSERSLIST_ENV=kaios3 && parcel build --no-source-maps --no-cache --target kaios3",
    "clean": "rimraf ./dist",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^3.0.0",
    "date-fns": "^2.25.0",
    "foxcasts-core": "^3.1.0",
    "lodash.throttle": "^4.1.1",
    "onyx-ui": "^0.5.2",
    "parcel": "^2.4.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "parcel-transformer-svelte3-plus": "github:stengland/parcel-transformer-svelte3-plus",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-spa-router": "^3.2.0",
    "typescript": "^4.6.3",
    "xml-formatter": "^2.5.1",
    "yargs": "^17.2.1"
  },
  "browserslist": {
    "kaios2": "Firefox 48",
    "kaios3": "Firefox 84"
  },
  "targets": {
    "kaios2": {
      "source": "src/index.html"
    },
    "kaios3": {
      "source": "src/index.html"
    }
  }
}
