{
  "name": "mica",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "npm run copy && node build/esbuild_v3.mjs",
    "build:watch": "npm run copy && node build/esbuild_v3.mjs --watch",
    "dev": "npm run clean && npm run copy && node build/esbuild_v3.mjs --serve",
    "build:kaios2": "npm run clean && rollup -c",
    "dev:kaios2": "npm run clean && rollup -c -w",
    "start": "sirv dist --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prebuild": "npm run clean && npm run copy",
    "predev": "npm run clean && npm run copy",
    "clean": "rimraf dist",
    "copy": "mkdir -p dist && cp -R public/* dist"
  },
  "dependencies": {
    "date-fns": "^2.25.0",
    "foxcasts-core": "^3.1.0",
    "lodash.throttle": "^4.1.1",
    "sirv-cli": "^1.0.0",
    "svelte-spa-router": "^3.2.0",
    "xml-formatter": "^2.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-block-scoping": "^7.16.0",
    "@babel/plugin-transform-for-of": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "esbuild": "^0.13.15",
    "esbuild-plugin-babel": "^0.2.3",
    "esbuild-svelte": "^0.5.7",
    "live-server": "^1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.7.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "yargs": "^17.2.1"
  }
}
